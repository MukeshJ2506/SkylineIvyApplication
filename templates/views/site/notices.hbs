{{!< default}}
	<h3>Notices at IvyLeague</h3>
            {{#ifeq data.notices.length 0}}<h5 class="text-weight-normal">There are no notices to be displayed now.</h5>{{/ifeq}}
			{{# each data.notices}}
            <div class="panel panel-primary">
                <div class="panel-heading"> 
                    <h3 class="panel-title">Subject: {{name}}</h3> 
                </div> 
                <div class="panel-body"> 
                    <h5><strong>Start date of Event:</strong> {{formatDate startDate}}</h5>
                    <h5><strong>End date of Event:</strong> {{formatDate endDate}}</h5>
                    <h5><strong>Notice Description: </strong></h5>
                    <div>{{{description}}}</div> 
                </div> 
            </div>
            {{/each}}
     {{#ifeq data.notices.length 0}}{{else}}{{>pagination}}{{/ifeq}}